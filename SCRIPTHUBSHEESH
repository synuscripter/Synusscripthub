
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Synus Hub v1 ", "DarkTheme")
    
    local main = Window:NewTab("Main scripts")
    local MainSection = main:NewSection("Main")
    
    
    MainSection:NewButton("invisible fling", "Makes you fling people invisible!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/synuscripter/invisiblefling/main/invisiblefling'))()
    end)
    
    MainSection:NewButton("Fly fe", "Makes you fly Press E then tab!", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/synuscripter/synus-gui-free-/main/Synus%20fly%20(fling)'))()
   end)
        
   MainSection:NewButton("Teleport!", "Click on your mouse where you wanna teleport", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/synuscripter/Synus-teleport/main/teleportmouseclicksynus'))()
end)
    

MainSection:NewButton("Speed Super", "Makes you SUPER  speed", function()
 loadstring(game:HttpGet('https://raw.githubusercontent.com/synuscripter/Speed-gui/main/SUPERSPEED'))()
end)
    



local main = Window:NewTab("Headless korblox")
local MainSection = main:NewSection("Avatar gui")



MainSection:NewButton("Headless korblox", "Avatar gui made by rodizzy#0001", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/synuscripter/headlessgui/main/avatarguisynus'))()
end)
    
